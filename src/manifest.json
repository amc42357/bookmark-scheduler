{
    "manifest_version": 3,
    "name": "Bookmark Scheduler",
    "version": "1.0",
    "description": "Schedule your bookmarks to open at specific times",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "storage",
        "notifications",
        "alarms",
        "clipboardWrite"
    ]
  }